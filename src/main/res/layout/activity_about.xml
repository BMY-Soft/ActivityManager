<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:paddingLeft="@dimen/activity_horizontal_margin"
		android:paddingRight="@dimen/activity_horizontal_margin"
		android:paddingTop="@dimen/activity_vertical_margin"
		android:paddingBottom="@dimen/activity_vertical_margin"
		tools:context="org.libdll.so.am.AboutActivity">

	<TextView
	  android:layout_width="wrap_content"
	  android:layout_height="wrap_content"
	  android:textAppearance="?android:attr/textAppearanceLarge"
	  android:text="@string/org_and_app_name"
	  android:id="@+id/text_name"
	  android:layout_alignParentTop="true"
	  android:layout_alignParentLeft="true"
	  android:layout_alignParentStart="true"/>

	<TextView
	  android:layout_width="wrap_content"
	  android:layout_height="wrap_content"
	  android:textAppearance="?android:attr/textAppearanceLarge"
	  android:text="Version Unknown"
	  android:id="@+id/text_version"
	  android:layout_below="@+id/text_name"
	  android:layout_alignParentLeft="true"
	  android:layout_alignParentStart="true"/>

	<TextView
	  android:layout_width="wrap_content"
	  android:layout_height="wrap_content"
	  android:text="@string/license_text"
	  android:id="@+id/text_license"
	  android:layout_below="@+id/text_copyright"
	  android:layout_alignParentLeft="true"
	  android:layout_alignParentStart="true"/>

	<TextView
	  android:layout_width="wrap_content"
	  android:layout_height="wrap_content"
	  android:textAppearance="?android:attr/textAppearanceMedium"
	  android:text="@string/copyright_text"
	  android:id="@+id/text_copyright"
	  android:layout_below="@+id/text_version"
	  android:layout_alignParentLeft="true"
	  android:layout_alignParentStart="true"/>
</RelativeLayout>
